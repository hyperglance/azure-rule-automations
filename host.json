{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information",
      "Function":"Trace",
      "Host.Aggregator":"Trace",
      "Host.Results":"Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "functionTimeout": "00:10:00"
}
